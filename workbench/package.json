{
  "name": "ProjectSkeleton",
  "version": "1.5.0",
  "description": "Just a quick react / typescript ready dev server for front-end dev.",
  "main": "server.js",
  "scripts": {
    "serve": "browser-sync start --server ./www --files ./www --config ./config/browsersync.config.js",
    "start": "parallelshell -v \"webpack --config config/webpack.config.dev.js --watch\" \"npm run serve\"",
    "build-dev": "webpack --config config/webpack.config.dev.js",
    "build": "webpack --config config/webpack.config.prod.js",
    "deploy": "surge -p www -d armstrong-bench.surge.sh"
  },
  "author": "Rocketmakers",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "autoprefixer-loader": "^3.1.0",
    "browser-sync": "^2.11.1",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "node-sass": "3.8.0",
    "parallelshell": "^2.0.0",
    "postcss-loader": "^0.8.2",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.3",
    "typings": "1.3.0",
    "underscore": "1.8.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2"
  },
  "dependencies": {
    "@types/classnames": "0.0.30",
    "@types/es6-promise": "0.0.32",
    "@types/history": "^2.0.37",
    "@types/moment": "^2.13.0",
    "@types/node": "^6.0.40",
    "@types/react": "^0.14.36",
    "@types/react-dom": "^0.14.17",
    "@types/react-motion": "0.0.20",
    "@types/react-router": "^2.0.36",
    "@types/underscore": "^1.7.33",
    "classnames": "2.2.5",
    "es6-promise": "^3.3.1",
    "moment": "2.14.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "2.6.1"
  }
}

{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "isolatedModules": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "jsx": "react",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "sourceMap": false,
    "noLib": false,
    "preserveConstEnums": false,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "../www",
    "moduleResolution": "node"
  },
  "exclude": [
    "../armstrong/node_modules/**"
  ]
}